{
    "manifest_version": 2,
    "name": "Password Generator",
    "version": "1.0",
    "description": "Generate secure passwords with ease.",
    "icons": {
      "128": "favicon.png"
    },
    "browser_action": {
      "default_popup": "index.html",
      "default_icon": "favicon.png"
    },

    "permissions": [
      "activeTab",
      "storage"
    ]
  }
  
  